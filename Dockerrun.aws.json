{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name":"client",
            "image": "classicaddetz/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"server",
            "image": "classicaddetz/multi-server",
            "hostname":"api",
            "essential":false
        },         
        {
            "name":"worker",
            "image": "classicaddetz/multi-worker",
            "hostname":"worker",
            "essential":false
        },        
        {
            "name":"nginx",
            "image": "classicaddetz/multi-nginx",
            "essential":true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client", "server"]
        }
    ]
}